{
  "name": "security-testing-demo",
  "version": "1.0.0",
  "description": "A vulnerable web application for demonstrating security testing skills",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "npm run test:api && npm run test:e2e",
    "test:api": "playwright test test/api --project=api",
    "test:e2e": "playwright test test/e2e --project=chromium",
    "test:ml-anomaly": "playwright test test/ml-anomaly --project=ml-anomaly",
    "test:all": "npm run test && npm run test:ml-anomaly",
    "test:results": "node generate-test-results.js",
    "test:report": "npx playwright show-report"
  },
  "keywords": [
    "security",
    "owasp",
    "penetration-testing",
    "vulnerabilities"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^11.0.0",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.19.30",
    "nodemon": "^3.0.1",
    "typescript": "^5.0.0",
    "cross-env": "^7.0.3"
  }
}
